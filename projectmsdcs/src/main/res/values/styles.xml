<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="EditTextMsdcs" parent="android:Widget.EditText">
        <item name="android:background">@drawable/msdcs_edit_text_holo_light</item>
        <item name="android:textColor">#000000</item>
    </style>

    <style name="AutoCompleteTextViewMsdcs" parent="android:Widget.AutoCompleteTextView">
        <item name="android:dropDownSelector">@drawable/msdcs_list_selector_holo_light</item>
        <item name="android:background">@drawable/msdcs_edit_text_holo_light</item>
        <item name="android:textColor">#000000</item>
    </style>

    <style name="ButtonMsdcs" parent="android:Widget.Button">
        <item name="android:background">@drawable/msdcs_btn_default_holo_light</item>
        <item name="android:minHeight">48dip</item>
        <item name="android:minWidth">64dip</item>
        <item name="android:textColor">#000000</item>
    </style>

    <style name="ImageButtonMsdcs" parent="android:Widget.ImageButton">
        <item name="android:background">@drawable/msdcs_btn_default_holo_light</item>
    </style>

    <style name="ProgressBarMsdcs" parent="android:Widget.ProgressBar.Horizontal">
        <item name="android:progressDrawable">@drawable/msdcs_progress_horizontal_holo_light</item>
        <item name="android:indeterminateDrawable">@drawable/msdcs_progress_indeterminate_horizontal_holo_light</item>
        <item name="android:minHeight">16dip</item>
        <item name="android:maxHeight">16dip</item>
    </style>

    <style name="SpinnerMsdcs" parent="android:Widget.Spinner">
        <item name="android:background">@drawable/msdcs_spinner_background_holo_light</item>
        <item name="android:dropDownSelector">@drawable/msdcs_list_selector_holo_light</item>
    </style>

    <style name="SpinnerDropDownItemMsdcs" parent="android:Widget.DropDownItem.Spinner">
        <item name="android:checkMark">@drawable/msdcs_btn_radio_holo_light</item>
    </style>

    <style name="AutoCompleteTextViewmsdcs" parent="android:Widget.Holo.Light.AutoCompleteTextView">
        <item name="android:dropDownSelector">@drawable/msdcs_list_selector_holo_light</item>
        <item name="android:background">@drawable/msdcs_edit_text_holo_light</item>
    </style>

    <style name="Buttonmsdcs" parent="android:Widget.Holo.Light.Button">
        <item name="android:background">@drawable/msdcs_btn_default_holo_light</item>
    </style>

    <style name="ImageButtonmsdcs" parent="android:Widget.Holo.Light.ImageButton">
        <item name="android:background">@drawable/msdcs_btn_default_holo_light</item>
    </style>

    <style name="Spinnermsdcs" parent="android:Widget.Holo.Light.Spinner">
        <item name="android:background">@drawable/msdcs_spinner_background_holo_light</item>
        <item name="android:dropDownSelector">@drawable/msdcs_list_selector_holo_light</item>
    </style>

    <style name="Tabmsdcs">
        <item name="android:gravity">center_horizontal</item>
        <item name="android:paddingLeft">16dip</item>
        <item name="android:paddingRight">16dip</item>
        <item name="android:background">@drawable/msdcs_tab_indicator_holo</item>
        <item name="android:layout_width">0dip</item>
        <item name="android:layout_weight">1</item>
        <item name="android:minWidth">80dip</item>
    </style>

    <style name="TabTextmsdcs">
        <item name="android:textColor">#000000</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textStyle">bold</item>
        <!-- v14 <item name="android:textAllCaps">true</item> -->
        <item name="android:ellipsize">marquee</item>
        <item name="android:maxLines">2</item>
        <item name="android:maxWidth">180dip</item>
    </style>

    <style name="ToggleMsdcs" parent="android:Widget.Holo.Light.Button.Toggle">
        <item name="android:background">@drawable/msdcs_btn_toggle_holo_light</item>
    </style>

</resources>